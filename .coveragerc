[run]
source = .
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*
    */node_modules/*
    */android_app/*
    */vscode_extension/*
    */ui/*
    */assets/*
    */agent/*
    adapters/roo_adapter.py
    setup.py
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
